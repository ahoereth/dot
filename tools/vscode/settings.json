{
    "editor.renderWhitespace": "boundary",
    "editor.rulers": [
        80,
        88,
        100
    ],
    "editor.wordWrapColumn": 80,
    "files.trimTrailingWhitespace": true,
    "python.pythonPath": "/usr/local/miniconda3/envs/py36/bin/python",
    "python.linting.pylintPath": "/usr/local/miniconda3/envs/py36/bin/pylint",
    "python.linting.pycodestylePath": "/usr/local/miniconda3/envs/py36/bin/pycodestyle",
    "python.linting.mypyPath": "/usr/local/miniconda3/bin/mypy",
    "python.linting.pycodestyleEnabled": false,
    "python.linting.pylintEnabled": false,
    "terminal.integrated.shell.linux": "/bin/zsh",
    "terminal.integrated.shell.osx": "/bin/zsh",
    "latex-workshop.latex.autoBuild.cleanAndRetry.enabled": false,
    "latex-workshop.latex.autoBuild.onSave.enabled": false,
    "latex-workshop.view.pdf.external.command": {
        "command": "displayfile",
        "args": [
            "-r",
            "%PDF%"
        ]
    },
    "latex-workshop.view.pdf.external.synctex": {
        "command": "displayline",
        "args": [
            "-r",
            "%LINE%",
            "%PDF%",
            "%TEX%"
        ]
    },
    "latex-workshop.message.update.show": false,
    "editor.minimap.enabled": false,
    "window.zoomLevel": 1,
    "workbench.startupEditor": "newUntitledFile",
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "html.format.endWithNewline": true,
    "terraform.codelens.enabled": false,
    "files.associations": {
        "*.tf.tmpl": "tf",
        "Dockerfile.*": "dockerfile",
        "*.yml.tmpl": "yaml",
        "*.yaml.tmpl": "yaml",
        "azure-pipelines.yml": "azure-pipelines",
        "*.tpl": "yaml",
        "*.yml": "azure-pipelines",
        "*.spec": "python"
    },
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/tmp": true,
        "**/node_modules": true,
        "**/bower_components": true,
        "**/dist": true
    },
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/**": true,
        "**/tmp/**": true,
        "**/bower_components/**": true,
        "**/dist/**": true
    },
    "python.envFile": "${workspaceFolder}/.venv",
    "extensions.ignoreRecommendations": true,
    "editor.renderWhitespace": "all",
    "editor.useTabStops": false,
    "python.formatting.blackPath": "/usr/local/miniconda3/bin/black",
    "python.formatting.autopep8Path": "/usr/local/miniconda3/envs/py36/bin/autopep8",
    "python.formatting.yapfPath": "/usr/local/miniconda3/envs/py36/bin/yapf",
    "python.formatting.provider": "black",
    "latex-workshop.latex.autoBuild.run": "never",
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "python.jediPath": "/usr/local/miniconda3/envs/py36/lib/python3.6/site-packages/jedi",
    "python.linting.flake8Path": "/usr/local/miniconda3/bin/flake8",
    "latex-workshop.docker.image.latex": "chktex",
    "editor.wordWrap": "on",
    "workbench.colorTheme": "Visual Studio Light",
    "[cpp]": {
        "editor.defaultFormatter": "xaver.clang-format"
    },
    "search.useGlobalIgnoreFiles": true,
    "gitblame.ignoreWhitespace": true,
    "gitblame.statusBarPositionPriority": 1,
    "explorer.confirmDragAndDrop": false,
    "python.linting.mypyArgs": [
        "--ignore-missing-imports",
        "--follow-imports=normal",
        "--show-column-numbers"
    ],
    "[azure-pipelines]": {},
    "python.dataScience.useNotebookEditor": false,
    "vsicons.dontShowNewVersionMessage": true,
    "python.linting.pylintUseMinimalCheckers": false,
    "clang-format.language.javascript.enable": false,
    "python.linting.lintOnSave": false,
    "standard.run": "onSave",
    "python.linting.enabled": false,
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.formatOnSave": true,
    "javascript.format.enable": false,
    "javascript.validate.enable": false,
    "javascript.format.insertSpaceAfterSemicolonInForStatements": false,
    "typescript.validate.enable": false
}
